{
  "name": "react-redux-mirror",
  "version": "0.0.6",
  "description": "A Higher Order Component using react-redux to temporary mirror parts of a Redux store",
  "main": "dist/react-redux-mirror.js",
  "module": "dist/react-redux-mirror.es.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "eslint src/**",
    "test": "env NODE_ENV=test mocha -r jsdom-global/register --compilers js:babel-register test/*.test.js",
    "test-tdd": "env NODE_ENV=test mocha --watch -r jsdom-global/register --compilers js:babel-register test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loanmarket/react-redux-mirror.git"
  },
  "keywords": [
    "redux",
    "react",
    "form",
    "mirror",
    "hoc"
  ],
  "author": "Benjamin Forster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loanmarket/react-redux-mirror/issues"
  },
  "homepage": "https://github.com/loanmarket/react-redux-mirror#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.4.2",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-config-loanmarket-react": "^1.0.5",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "invariant": "^2.2.2",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.mapvalues": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "mocha": "^5.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "reduce-reducers": "^0.1.2",
    "redux-mock-store": "^1.3.0",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-buble": "^0.17.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sinon": "^4.0.1"
  },
  "dependencies": {
    "immutability-helper": "^2.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1"
  }
}
